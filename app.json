{
  "expo": {
    "name": "TransLink",
    "slug": "translink-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.translink.app",
      "config": {
        "googleMapsApiKey": "AIzaSyDXLG6TnuK9HeawdmSVXA56KTKdVul3jLE"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.translink.app",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_BACKGROUND_LOCATION",
        "android.permission.FOREGROUND_SERVICE",
        "android.permission.FOREGROUND_SERVICE_LOCATION",
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.WAKE_LOCK"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDXLG6TnuK9HeawdmSVXA56KTKdVul3jLE"
        }
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow TransLink to track your location for real-time bus tracking and route guidance.",
          "isAndroidBackgroundLocationEnabled": true,
          "isIosBackgroundLocationEnabled": true
        }
      ],
      [
        "expo-task-manager"
      ],
      [
        "expo-background-fetch",
        {
          "backgroundFetchIntervalSeconds": 30
        }
      ],
      "expo-secure-store"
    ],
    "scheme": "translink",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "99360d4d-e9b6-413d-ac8c-a57cb5ebe4f9"
      },
      "googleAuth": {
        "webClientId": "135721186146-h6gtdbff572112jipu5ejf4aevst7gob.apps.googleusercontent.com",
        "androidClientId": "",
        "iosClientId": "",
        "expoClientId": "135721186146-h6gtdbff572112jipu5ejf4aevst7gob.apps.googleusercontent.com"
      }
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "owner": "chamika200273"
  }
}
